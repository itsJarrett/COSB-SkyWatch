load("render.star", "render")
load("http.star", "http")
load("encoding/base64.star", "base64")

OPENSKY_API_URL = "https://opensky-network.org/api/states/all"

PLANE_ICON = base64.decode("""

""")

# Bounding box for a 5-mile radius
LATITUDE_MIN = "33.700515"   # 33.7414 - 0.145
LONGITUDE_MIN = "-118.136057" # -118.1048 - 0.189
LATITUDE_MAX = "33.788996"   # 33.7414 + 0.145
LONGITUDE_MAX = "-118.016262" # -118.1048 + 0.189

def main():
    rep = http.get(OPENSKY_API_URL, params={"lamin": LATITUDE_MIN, "lomin": LONGITUDE_MIN, "lamax": LATITUDE_MAX, "lomax": LONGITUDE_MAX})
    if rep.status_code != 200:
        fail("OpenSky request failed with status %d", rep.status_code)
    
    plane_data = rep.json()["states"]
    
    if plane_data != None and len(plane_data) > 0:
        children = [render.Image(src=PLANE_ICON, width=40, height=10)]
        for i in range(min(3, len(plane_data))):
            plane = plane_data[i]
            tail_number = plane[1]
            altitude = plane[7]
            velocity = plane[9]
            children.append(
                render.Marquee(
                    width=64,
                    child=render.Text("Plane %d: Tail Number: %s, Altitude: %s ft, Velocity: %s m/s" % (i+1, tail_number, altitude, velocity), font="5x8", height=7),
                    offset_start=5,
                    offset_end=32,
                )
            )
            
        return render.Root(
            child=render.Box(
                render.Column(
                    expanded=True,
                    main_align="space_evenly",
                    cross_align="center",
                    children=children,
                ),
            ),
        )
    else:
        return render.Root(
            child=render.Box(
                render.Column(
                    expanded=True,
                    main_align="center",
                    cross_align="center",
                    children=[
                        render.Text("No", font="5x8", height=7, color="#969696"),
                        render.Image(src=PLANE_ICON, width=40, height=10),
                        render.Text("Overhead :(", font="5x8", height=7, color="#969696")
                    ],
                ),
            ),
        )
